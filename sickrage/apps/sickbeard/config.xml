  <ConfigURL></ConfigURL>
  <ReservePort>8888</ReservePort>
  <DebianPackage>sickrage</DebianPackage>
  <ServiceName>fvapp-sickrage.service</ServiceName>
  <Description lang="en-us">SickRage: Automatic library manager</Description>
</Application>
